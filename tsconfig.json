{
  "compilerOptions": {
    "target": "esnext",
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noEmit": true,
    "moduleResolution": "Node16",
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "Node16",
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src"],
  "exclude": ["node_modules/", "./api/*"]
}
